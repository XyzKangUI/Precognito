<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

    <Script file="CompactUnitFrame.lua"/>

    <Frame name="CompactUnitFrameTemplate2" setAllPoints="true" virtual="true">
        <Layers>
            <Layer level="BORDER" textureSubLevel="5">
                <Texture name="$parentMyHealPredictionBars" parentKey="myHealPredictionBar"/>
                <Texture name="$parentOtherHealPredictionBar" parentKey="otherHealPredictionBar"/>
                <Texture name="$parentTotalAbsorbBar" parentKey="totalAbsorbBar"/>
            </Layer>
            <Layer level="ARTWORK" textureSubLevel="6">
                <Texture name="$parentTotalAbsorbOverlay" parentKey="totalAbsorbOverlayBar"/>
            </Layer>
            <Layer level="BORDER" textureSubLevel="1">
                <Texture name="$parentMyHealAbsorb" parentKey="myHealAbsorbBar"/>
                <Texture name="$parentMyHealAbsorbLeftShadow" parentKey="myHealAbsorbBarLeftShadow"
                         file="Interface\RaidFrame\Absorb-Edge"/>
                <Texture name="$parentMyHealAbsorbRightShadow" parentKey="myHealAbsorbBarRightShadow"
                         file="Interface\RaidFrame\Absorb-Edge">
                    <TexCoords left="1" right="0" top="0" bottom="1"/>
                </Texture>
            </Layer>
            <Layer level="BORDER" textureSubLevel="2">
                <Texture name="$parentOverAbsorbGlow" parentKey="overAbsorbGlowBar"/>
                <Texture name="$parentOverHealAbsorbGlow" parentKey="overHealAbsorbGlowBar"/>
            </Layer>
        </Layers>
    </Frame>

</Ui>